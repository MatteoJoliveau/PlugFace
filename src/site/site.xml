            <item name="JavaDocs" href="/apidocs/index.html" target="_blank"/>
            <item name="Issue Tracking" href="https://github.com/MatteoJoliveau/PlugFace/issues" target="_blank"/>
            <item name="Project Roadmap" href="https://github.com/MatteoJoliveau/PlugFace/projects" target="_blank"/>
            <item name="SonarQube Quality Check" href="https://sonarqube.vault101.ga/dashboard?id=org.plugface%3Aplugface"/>
        </menu>
        <menu name="Languages">
            <item name="Italian" href="../it/"/>
            <item name="French" href="../fr/"/>
            <item name="German" href="../de/"/>
            <item name="Spanish" href="../es/"/>
